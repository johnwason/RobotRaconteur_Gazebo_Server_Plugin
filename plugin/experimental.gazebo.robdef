object ImuSensor
	implements Sensor
	property string Name [readonly]
	property string Type [readonly]
	property string ParentName [readonly]
	property Pose Pose [readonly]
	
	property uint8 Active
	property double UpdateRate
	property double LastUpdateTime [readonly]
	property double LastMeasurementTime [readonly]
	
	function void SetReferencePose()
	
	property ImuState ReadState [readonly]
	wire ImuState StateWire [readonly]
end object

object Light
	property string Name [readonly]
	property string Type [readonly]
	property Pose Pose [readonly]
	property double[3] Direction [readonly]
	
	property Color DiffuseColor
	property Color SpecularColor
end object