object ForceTorqueSensor
	implements Sensor
	property string Name [readonly]
	property string Type [readonly]
	property string ParentName [readonly]
	property Pose Pose [readonly]
	
	property uint8 Active
	property double UpdateRate
	property double LastUpdateTime [readonly]
	property double LastMeasurementTime [readonly]
	
	property double[6] ForceTorque [readonly]
	wire double[6] ForceTorqueWire [readonly]
end object

object GpsSensor
	implements Sensor
	property string Name [readonly]
	property string Type [readonly]
	property string ParentName [readonly]
	property Pose Pose [readonly]
	
	property uint8 Active
	property double UpdateRate
	property double LastUpdateTime [readonly]
	property double LastMeasurementTime [readonly]
	
	property GpsState ReadState [readonly]
	wire GpsState StateWire [readonly]
